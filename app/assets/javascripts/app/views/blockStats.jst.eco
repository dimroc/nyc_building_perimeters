<table class='table'>
  <thead>
    <tr>
      <th>Zip Code</th>
      <th>Neighborhood</th>
      <th>Director</th>
      <th>Recorded</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @zip_code %></td>
      <td><%= @neighborhood %></td>
      <td>
        <img class="profile" src='<%= @userPhoto() %>' alt='<%=@userName()%>'></img>
        <%= @userName() %>
      </td>
      <td><%= @recorded() %></td>
    </tr>
  </tbody>
</table>
